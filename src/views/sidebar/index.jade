md-sidenav.md-sidenav-left&attributes({
  layout: "column",
  "md-component-id": "left",
  "md-is-locked-open": "$mdMedia('gt-sm')"
})
  div(ng-controller="SidebarController")
    md-menu-item.menu-header
      p Avatar
    md-menu-item(ng-repeat="item in menuItems")
      md-button(ng-href="#/{{item.url}}")
        md-icon(md-menu-align-target) {{item.icon}}
        | {{item.name | translate}}
